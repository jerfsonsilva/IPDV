$alertify.success(response.data);\n            })\n            .catch((err) => {\n              if (err.response != undefined) {\n                var msg = err.response.data.err;\n                $this.$alertify.error(msg);\n              }\n            });\n        }\n      );\n      \n    },\n  },\n  components: {\n    Navbar,\n  },\n};\n</script>\n\n"],"sourceRoot":"src/views/cargo"}]}