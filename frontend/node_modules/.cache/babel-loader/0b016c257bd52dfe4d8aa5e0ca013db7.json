AA,SAAA,CAAA,OAAA,CACA,8CADA,EAEA,YAAA;AACA,QAAA,KAAA,CACA,MADA,CACA,GAAA,CAAA,eAAA,GAAA,QAAA,GAAA,EADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,GAAA,EAAA,CAAA,EAFA,CAEA;;AACA,UAAA,KAAA,CAAA,SAAA,CAAA,OAAA,CAAA,QAAA,CAAA,IAAA;AACA,SANA,EAOA,KAPA,CAOA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,QAAA,IAAA,SAAA,EAAA;AACA,gBAAA,GAAA,GAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,GAAA;AACA,YAAA,KAAA,CAAA,SAAA,CAAA,KAAA,CAAA,GAAA;AACA;AACA,SAZA;AAaA,OAhBA;AAkBA;AA3BA,GAfA;AA4CA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA;AA5CA,CAAA","sourcesContent":["<template>\n  <div>\n    <Navbar :page=\"'user'\" />\n\n    <div class=\"container\">\n      <div class=\"card\">\n        <header class=\"card-header\">\n          <p class=\"card-header-title\">Lista de usuarios</p>\n          <a class=\"button is-info is-small mt-2\" href=\"/user/novo\"\n            >Cadastrar +\n          </a>\n        </header>\n        <div class=\"card-content\">\n          <div class=\"column is-offset-9 is-one-quarter\">\n            <select\n              class=\"select input\"\n              required=\"\"\n              placeholder=\"Departamento\"\n              v-model=\"FKIDdepartamento\"\n              @change=\"buscarUsuarios()\"\n            >\n              <option value=\"\" selected>Departamento...</option>\n              <option\n                v-for=\"departamento in listaDepartamento\"\n                :value=\"departamento.id\"\n                :key=\"departamento.id\"\n              >\n                {{ departamento.descricao }}\n              </option>\n            </select>\n          </div>\n\n          <table class=\"table is-fullwidth\">\n            <thead>\n              <td>Nome</td>\n              <td>Email</td>\n              <td></td>\n              <td></td>\n            </thead>\n            <tbody>\n              <tr v-for=\"(item, key) in lista\" :key=\"key\">\n                <td>{{ item.nome }}</td>\n                <td>{{ item.email }}</td>\n                <td class=\"editar\">\n                  <a :href=\"'/user/' + item.id\">Editar </a>\n                </td>\n                <td class=\"excluir\" @click=\"excluir(item.id, key)\">Excluir</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport env from \"../../../env\";\nimport Navbar from \"../../components/layout/Navbar.vue\";\n\nexport default {\n  data() {\n    return {\n      lista: [],\n      listaDepartamento: [],\n      FKIDdepartamento: \"\",\n    };\n  },\n  mounted() {\n    axios.get(env.urlServidorBack + \"/departamento\").then((response) => {\n      //Buscar departamentos\n      this.listaDepartamento = response.data;\n    });\n    this.buscarUsuarios();\n  },\n  methods: {\n    buscarUsuarios() {\n      axios.get(env.urlServidorBack + \"/user\"+(this.FKIDdepartamento!=\"\"?\"?FKIDdepartamento=\"+this.FKIDdepartamento:\"\")).then((response) => {\n        //Buscar servidores\n        this.lista = response.data;\n      });\n    },\n    excluir(id, key) {\n      var $this = this;\n      this.$alertify.confirm(\n        \"Tem certeza que deseja excluir esse usuario?\",\n        function () {\n          axios\n            .delete(env.urlServidorBack + \"/user/\" + id)\n            .then((response) => {\n              console.log(response);\n              $this.lista.splice(key, 1); //Remover elemento da lista\n              $this.$alertify.success(response.data);\n            })\n            .catch((err) => {\n              if (err.response != undefined) {\n                var msg = err.response.data.err;\n                $this.$alertify.error(msg);\n              }\n            });\n        }\n      );\n    },\n  },\n  components: {\n    Navbar,\n  },\n};\n</script>\n\n"],"sourceRoot":"src/views/usuario"}]}