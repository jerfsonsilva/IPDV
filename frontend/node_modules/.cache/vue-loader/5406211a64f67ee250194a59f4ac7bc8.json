        <form\n            method=\"POST\"\n            ref=\"formVinculo\"\n            @submit=\"cadastrarEditar\"\n            enctype=\"multipart/form-data\"\n          >\n            <div class=\"\">\n              <div class=\"columns\">\n                <div class=\"column is-two-fifths\">\n                  <label class=\"pt-4\">\n                    Nome\n                    <input\n                      required\n                      type=\"text\"\n                      class=\"input\"\n                      placeholder=\"Nome do usuÃ¡rio\"\n                      name=\"nome\"\n                      v-model=\"form.nome\"\n                    />\n                  </label>\n                </div>\n                <div class=\"column\">\n                  <label class=\"pt-4\">\n                    E-mail\n                    <input\n                      required\n                      type=\"email\"\n                      class=\"input\"\n                      placeholder=\"Email\"\n                      name=\"email\"\n                      v-model=\"form.email\"\n                    />\n                  </label>\n                </div>\n              </div>\n              <div class=\"columns\">\n                <div class=\"column\">\n                  <label class=\"pt-4\">\n                    Cargo\n\n                    <select\n                      name=\"FKIDcargo\"\n                      class=\"select input\"\n                      required=\"\"\n                      v-model=\"form.FKIDcargo\"\n                      placeholder=\"selecione\"\n                    >\n                      <option value=\"\" selected>Selecione...</option>\n                      <option\n                        v-for=\"cargo in cargos\"\n                        :value=\"cargo.id\"\n                        :key=\"cargo.id\"\n                      >\n                        {{ cargo.descricao }}\n                      </option>\n                    </select>\n                  </label>\n                </div>\n                <div class=\"column\">\n                  <label class=\"pt-4\">\n                    Departamento\n\n                    <select\n                      name=\"FKIDdepartamento\"\n                      class=\"select input\"\n                      required=\"\"\n                      v-model=\"form.FKIDdepartamento\"\n                    >\n                      <option value=\"\" selected>Selecione...</option>\n                      <option\n                        v-for=\"departamento in departamentos\"\n                        :value=\"departamento.id\"\n                        :key=\"departamento.id\"\n                      >\n                        {{ departamento.descricao }}\n                      </option>\n                    </select>\n                  </label>\n                </div>\n              </div>\n              <div class=\"columns\">\n                <div class=\"column\">\n                  <label class=\"pt-4\">\n                    Senha\n                    <input\n                      required\n                      type=\"password\"\n                      class=\"input\"\n                      placeholder=\"Senha\"\n                      name=\"password\"\n                      v-model=\"form.password\"\n                    />\n                  </label>\n                </div>\n                <div class=\"column\">\n                  <label class=\"pt-4\">\n                    Confirme a Senha\n                    <input\n                      required\n                      type=\"password\"\n                      class=\"input\"\n                      placeholder=\"Confirmar a senha\"\n                      v-model=\"form.confirmarSenha\"\n                    />\n                  </label>\n                </div>\n                <button class=\"button is-success mp-37\" type=\"submit\">\n                  Salvar\n                </button>\n              </div>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport env from \"../../../env\";\nimport Navbar from \"../../components/layout/Navbar.vue\";\n\nexport default {\n  props: [\"id\"],\n  data() {\n    return {\n      form: {\n        nome: \"\",\n        password: \"\",\n        confirmarSenha: \"\",\n        email: \"\",\n        FKIDdepartamento: null,\n        FKIDcargo: null,\n      },\n      cargos: [],\n      departamentos: [],\n    };\n  },\n  mounted() {\n    if (this.id) {\n      //Editar\n      axios.get(env.urlServidorBack + \"/user/\" + this.id).then((response) => {\n        //Buscar usuario\n        this.form = response.data;\n      });\n    }\n    axios.get(env.urlServidorBack + \"/cargo\").then((response) => {\n      //Buscar cargos\n      this.cargos = response.data;\n    });\n\n    axios.get(env.urlServidorBack + \"/departamento\").then((response) => {\n      //Buscar departamentos\n      this.departamentos = response.data;\n    });\n  },\n  methods: {\n    cadastrarEditar(e) {\n      e.preventDefault();\n\n      if (this.form.password != this.form.confirmarSenha) {\n        this.$alertify.error(\"As senhas nÃ£o coinciden\");\n        return;\n      }\n      if (this.id) {\n        //Editar\n        axios\n          .put(env.urlServidorBack + \"/user\", this.form)\n          .then((res) => {\n            console.log(res);\n            this.$alertify.success(\"Editado com sucesso\");\n            this.$router.push({ name: \"ListaUsuario\" });\n          })\n          .catch((err) => {\n            var msg = err.response.data.err;\n            this.$alertify.error(msg);\n          });\n      } else {\n        axios\n          .post(env.urlServidorBack + \"/user\", this.form)\n          .then((res) => {\n            console.log(res);\n            this.$alertify.success(\"Cadastrado com sucesso\");\n            this.$router.push({ name: \"ListaUsuario\" });\n          })\n          .catch((err) => {\n            var msg = err.response.data.err;\n            this.$alertify.error(msg);\n          });\n      }\n    },\n  },\n  components: {\n    Navbar,\n  },\n};\n</script>\n\n"]}]}