               <label class=\"pt-4\">\n                    Descrição\n                    <input\n                      required\n                      type=\"text\"\n                      class=\"input\"\n                      placeholder=\"descrição\"\n                      name=\"descricao\"\n                      v-model=\"form.descricao\"\n                    />\n                  </label>\n                </div>\n          \n              </div>\n         \n              <div class=\"columns\">\n             \n                <button class=\"button is-success mp-37\" type=\"submit\">\n                  Salvar\n                </button>\n              </div>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport env from \"../../../env\";\nimport Navbar from \"../../components/layout/Navbar.vue\";\n\nexport default {\n  props: [\"id\"],\n  data() {\n    return {\n      form: {\n        descricao: \"\",\n      },\n    };\n  },\n  mounted() {\n    if (this.id) {\n      //Editar\n      axios.get(env.urlServidorBack + \"/centroCusto/\" + this.id).then((response) => {\n        //Buscar centroCusto\n        this.form = response.data;\n      });\n    }\n   \n  },\n  methods: {\n    cadastrarEditar(e) {\n      e.preventDefault();\n\n      if (this.id) {\n        //Editar\n        axios\n          .put(env.urlServidorBack + \"/centroCusto\", this.form)\n          .then((res) => {\n            console.log(res);\n            this.$alertify.success(\"Editado com sucesso\");\n            this.$router.push({ name: \"ListaCentroCusto\" });\n          })\n          .catch((err) => {\n            var msg = err.response.data.err;\n            this.$alertify.error(msg);\n          });\n      } else {\n        axios\n          .post(env.urlServidorBack + \"/centroCusto\", this.form)\n          .then((res) => {\n            console.log(res);\n            this.$alertify.success(\"Cadastrado com sucesso\");\n            this.$router.push({ name: \"ListaCentroCusto\" });\n          })\n          .catch((err) => {\n            var msg = err.response.data.err;\n            this.$alertify.error(msg);\n          });\n      }\n    },\n  },\n  components: {\n    Navbar,\n  },\n};\n</script>\n\n"]}]}